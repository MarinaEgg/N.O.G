{
    "version": 2,
    "functions": {
        "api/index.py": {
            "runtime": "python3.9",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/backend-api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/onboarding",
            "dest": "/api/index.py"
        },
        {
            "src": "/links/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/chat/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/",
            "dest": "/api/index.py"
        }
    ]
}
