{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 30
            }
        }
    ],
    "routes": [
        {
            "src": "/test",
            "dest": "/api/index.py"
        },
        {
            "src": "/debug",
            "dest": "/api/index.py"
        },
        {
            "src": "/backend-api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/onboarding/?",
            "dest": "/api/index.py"
        },
        {
            "src": "/links/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/chat/?.*",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}
